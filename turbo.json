{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "start": {
      "dependsOn": ["$IS_LOCAL"],
      "outputs": [".build/**", ".sst/**"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "packages/bootstrap#deploy": {
      "dependsOn": ["$GITHUB_REPOSITORY"],
      "outputs": []
    }
  }
}
